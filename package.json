{
  "name": "pre-entier-wms-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "dev": "nodemon server.js",
    "prettier": "npx prettier . --write",
    "docker:build": "docker build -t pre-entier-wms-backend:$(git rev-parse --short HEAD) .",
    "docker:run": "docker run -e PORT=3000 --rm --env-file .env -p 3000:3000 --name pre-entier-wms-backend pre-entier-wms-backend:$(git rev-parse --short HEAD)",
    "heroku:push": "heroku container:push web -a entier-wms-demo-backend",
    "heroku:release": "heroku container:release web -a entier-wms-demo-backend",
    "heroku:log": "heroku logs --tail -a entier-wms-demo-backend",
    "heroku:restart": "heroku restart -a entier-wms-demo-backend",
    "heroku:config": "heroku config -a entier-wms-demo-backend"
  },
  "engines": {
    "node": "20.17.0"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "1.13.2",
    "bcrypt": "5.1.1",
    "bcryptjs": "2.4.3",
    "chart.js": "4.4.8",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.9.7",
    "morgan": "1.10.1",
    "web-push": "3.6.7"
  },
  "devDependencies": {
    "nodemon": "3.1.11",
    "prettier": "3.6.2"
  }
